{
  "name": "nest-typeorm-graphql",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "12.10.0"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node -r ts-node/register -r tsconfig-paths/register dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dump": "ts-node src/dump"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.7",
    "@nestjs/core": "^7.6.7",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.7",
    "@nestjs/platform-socket.io": "^7.6.12",
    "@nestjs/platform-ws": "^7.6.12",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.12",
    "apollo-server-cache-memcached": "^0.6.7",
    "apollo-server-express": "^2.19.2",
    "bcrypt": "^5.0.0",
    "cache-manager": "^3.4.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "cloudinary": "^1.23.0",
    "compression": "^1.7.4",
    "dateformat": "^4.5.0",
    "express-rate-limit": "^5.2.3",
    "graphql": "^15.5.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-tools": "^7.0.2",
    "graphql-type-json": "^0.3.2",
    "handlebars": "^4.7.6",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "mongodb": "^3.6.3",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-facebook-token": "^4.0.0",
    "passport-google-plus-token": "^2.1.0",
    "passport-google-token": "^0.1.2",
    "passport-line-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.30",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.6.7",
    "@types/cache-manager": "^3.4.0",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "@types/socket.io": "^2.1.13",
    "@types/supertest": "2.0.10",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "tsc-watch": "4.2.9",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "typescript": "4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
